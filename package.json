{
  "name": "GerenciadorRDP",
  "version": "4.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@novnc/novnc": "^1.3.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "activedirectory2": "^2.2.0",
    "better-sqlite3": "^12.5.0",
    "electron-store": "^7.0.3",
    "guacamole-common-js": "^1.5.0",
    "guacamole-lite": "^1.2.0",
    "lucide-react": "^0.555.0",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "recharts": "^3.5.1",
    "web-vitals": "^2.1.4",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "wait-on": "^7.2.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "npm run electron:start",
    "electron:start": "concurrently \"npm:start\" \"wait-on http://localhost:3000 && electron .\"",
    "package": "npm run build:react && electron-builder",
    "build": "npm run build:react && electron-builder --win --x64"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.seu-nome.gerenciador-rdp",
    "productName": "Gerenciador de Conex√µes Enterprise",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "files": [
      "build/**/*",
      "public/**/*",
      "src/main/**/*"
    ],
    "extraResources": [
      {
        "from": "assets",
        "to": "assets"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "include": "installer/installer.nsh"
    }
  }
}